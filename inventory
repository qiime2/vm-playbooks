localhost ansible_python_interpreter=python

[jump_hosts]

[jump_hosts:vars]
ansible_ssh_private_key_file=../tmp/workshopkey-private.pem
ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'

[all_hosts]

[all_hosts:vars]
